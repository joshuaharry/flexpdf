{
  "compilerOptions": {
    "allowJs": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "module": "es2022",
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "es2022",
    "allowUnreachableCode": false
  },
  "include": ["src"]
}
